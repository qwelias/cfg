{
    "workbench.colorTheme": "Monokai Classic",
    "workbench.iconTheme": null,
    "debug.javascript.codelens.npmScripts": "never",
    "editor.lightbulb.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.fontFamily": "'Ubuntu Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 20,
    "editor.minimap.renderCharacters": false,
    "workbench.activityBar.visible": false,
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "breadcrumbs.icons": false,
    "workbench.editor.showIcons": false,
    "window.menuBarVisibility": "toggle",
    "outline.icons": false,
    "telemetry.telemetryLevel": "off",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/tests-coverage": true
    },
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "debug.allowBreakpointsEverywhere": true,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".tf",
                "cmd": "terraform fmt ${file}"
            }
        ]
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.startupEditor": "newUntitledFile",
    "javascript.suggest.autoImports": false,
    "typescript.suggest.autoImports": false,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.associations": {
        "*.env": "ini",
        "env_vars": "ini",
        "*.nomad": "hcl"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[caddyfile]": {
        "editor.defaultFormatter": "matthewpi.caddyfile-support"
    },
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "javascript.format.semicolons":"remove",
    "typescript.format.semicolons":"remove",
    "editor.bracketPairColorization.enabled": false,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terraform.experimentalFeatures.prefillRequiredFields": true,
    "terraform.experimentalFeatures.validateOnSave": true,
    "[hcl]": {
        "editor.defaultFormatter": "fredwangwang.vscode-hcl-format",
        "editor.formatOnSave": true
    },
    "cSpell.enabled": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "window.zoomLevel": 1,
    "security.workspace.trust.untrustedFiles": "open",
    "extensions.ignoreRecommendations": true
}