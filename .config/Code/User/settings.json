{
    "window.zoomLevel": 0.5,
    "workbench.colorTheme": "Monokai Classic",
    "workbench.iconTheme": null,
    "debug.javascript.codelens.npmScripts": "never",
    "npm.enableScriptExplorer": false,
    "editor.lightbulb.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.fontFamily": "'Ubuntu Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 18,
    "editor.minimap.renderCharacters": false,
    "workbench.activityBar.visible": false,
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "breadcrumbs.icons": false,
    "workbench.editor.showIcons": false,
    "window.menuBarVisibility": "toggle",
    "outline.icons": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "explorer",
    "gitlens.views.lineHistory.location": "explorer",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "gitlens.keymap": "none",
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/tests-coverage": true
    },
    "explorer.confirmDragAndDrop": false,
    "timeline.excludeSources": [
        "git-history"
    ],
    "gitlens.currentLine.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "rust.build_on_save": true,
    "debug.allowBreakpointsEverywhere": true,
    "rust-client.disableRustup": true,
    "rust.clippy_preference": "on",
    "typescript.updateImportsOnFileMove.enabled": "never",
    "eslint.run": "onSave",
    "eslint.format.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.options": {
        "ext": "ts,js"
    },
    "eslint.lintTask.options": "--ignore-path .gitignore .",
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.enabled": false,
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".tf",
                "cmd": "terraform fmt ${file}"
            }
        ]
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.startupEditor": "newUntitledFile",
    "cSpell.enabled": false,
    "javascript.suggest.autoImports": false,
    "typescript.suggest.autoImports": false
}