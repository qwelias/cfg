{
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Monokai Classic",
    "workbench.iconTheme": null,
    "editor.lightbulb.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.fontFamily": "'Ubuntu Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 17,
    "editor.minimap.renderCharacters": false,
    "workbench.activityBar.visible": false,
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "always",
    "breadcrumbs.icons": false,
    "workbench.editor.showIcons": false,
    "window.menuBarVisibility": "toggle",
    "outline.icons": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "explorer",
    "gitlens.views.lineHistory.location": "explorer",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "gitlens.keymap": "none",
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true
    },
    "explorer.confirmDragAndDrop": false,
    "timeline.excludeSources": [
        "git-history"
    ]
}